{
  "name": "redis.do",
  "version": "0.2.0",
  "description": "100% Redis-compatible database on Cloudflare Workers, Durable Objects, and SQLite",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "redis.do": "./dist/mcp/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp/index.d.ts",
      "import": "./dist/mcp/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsup",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "redis",
    "cloudflare",
    "workers",
    "durable-objects",
    "database",
    "serverless",
    "edge",
    "mcp",
    "ioredis"
  ],
  "author": "Nathan Clevenger",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/redis.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.0",
    "@cloudflare/workers-types": "^4.20250109.0",
    "@types/node": "^22.10.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0",
    "wrangler": "^4.0.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": ">=4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
